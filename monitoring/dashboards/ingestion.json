{
  "dashboard": {
    "panels": [
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "id": 1,
        "targets": [
          {
            "expr": "rate(akosha_ingestion_requests_total[1m])",
            "legendFormat": "{{system_id}}",
            "refId": "ingestionRate"
          }
        ],
        "title": "Uploads Per Minute",
        "type": "graph",
        "yAxes": [
          {
            "label": "Uploads / sec",
            "min": 0
          }
        ]
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "id": 2,
        "targets": [
          {
            "expr": "histogram_quantile(0.50, akosha_ingestion_duration_seconds)",
            "legendFormat": "P50",
            "refId": "p50Latency"
          }
        ],
        "title": "P50 Latency",
        "type": "graph",
        "yAxes": [
          {
            "label": "Latency (seconds)",
            "min": 0
          }
        ]
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "id": 3,
        "targets": [
          {
            "expr": "histogram_quantile(0.99, akosha_ingestion_duration_seconds)",
            "legendFormat": "P99",
            "refId": "p99Latency"
          }
        ],
        "title": "P99 Latency",
        "type": "graph",
        "yAxes": [
          {
            "label": "Latency (seconds)",
            "min": 0
          }
        ]
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "id": 4,
        "targets": [
          {
            "expr": "akosha_ingestion_queue_size",
            "legendFormat": "Queue Size",
            "refId": "queueSize"
          }
        ],
        "title": "Queue Size",
        "type": "graph",
        "yAxes": [
          {
            "label": "Uploads Waiting",
            "min": 0
          }
        ]
      },
      {
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "id": 5,
        "targets": [
          {
            "expr": "rate(akosha_ingestion_errors_total[1m])",
            "legendFormat": "{{error_type}}",
            "refId": "errorRate"
          }
        ],
        "title": "Error Rate",
        "type": "graph",
        "yAxes": [
          {
            "label": "Errors / sec",
            "min": 0
          }
        ]
      },
      {
        "fieldConfig": {
          "defaults": {
            "decimals": 2,
            "max": 100,
            "min": 0,
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "id": 6,
        "targets": [
          {
            "expr": "sum(rate(akosha_ingestion_requests_total{status='success'}[1m])) / sum(rate(akosha_ingestion_requests_total[1m]))",
            "legendFormat": "Success Rate",
            "refId": "successRate"
          }
        ],
        "title": "Success Rate",
        "type": "stat"
      }
    ],
    "refresh": "10s",
    "schemaVersion": 16,
    "tags": [
      "akosha",
      "ingestion"
    ],
    "timezone": "UTC",
    "title": "Akosha Ingestion Dashboard",
    "version": 0
  }
}
